#include<iostream>

using namespace std;

int main (){
  string s,a;
  cin>>s>>a;

  for(int i = 0;i<s.size();i++){
    if(s[i]<=92){
      s[i] += 32;
    }
  }

  for(int i = 0;i<a.size();i++){
    if(a[i]<=92){
      a[i] += 32;
    }
  }

  if(s<a) cout<<-1;
  else if(a<s) cout<<1;
  else {
    cout<<0;
  }





  




  return 0;
}

