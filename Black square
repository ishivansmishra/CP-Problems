#include<iostream>
#include<vector>
#include<string>


using namespace std;


int main (){
      int a,b,c,d;
      cin>>a>>b>>c>>d;

      int count = 0;
      string s;
      cin>>s;
      for(int i = 0;i<s.length();i++){
        if(s[i]=='1') count+=a;
        else if(s[i]=='2') count+=b;
        else if(s[i]=='3') count+=c;
        else count+=d;
      }
     cout<<count;
    return 0;
}
